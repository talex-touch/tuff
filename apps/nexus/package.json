    "@unocss/preset-typography": "^66.5.0",
    "@vite-pwa/nuxt": "^1.0.4",
    "@vueuse/core": "^13.5.0",
    "@vueuse/nuxt": "^13.5.0",
    "eslint": "^9.31.0",
    "eslint-plugin-format": "^1.0.1",
    "nitro-cloudflare-dev": "^0.2.2",
    "nuxt": "^4.0.0",
    "pinia": "^3.0.3",
    "typescript": "^5.8.3",
    "unocss": "^66.3.3",
    "vue": "^3.5.17",
    "vue-tsc": "^3.0.1",
