{
  "name": "@talex-touch/tuff-nexus",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "description": "Documentation for Tuff project",
  "scripts": {
    "build": "nuxt build",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxt dev",
    "dev": "nuxt dev --port 3200",
    "generate": "nuxt generate",
    "prepare": "nuxt prepare",
    "preview": "nuxt preview",
    "preview:cf": "pnpm build && npx wrangler pages dev .output/public --compatibility-date=2024-08-14",
    "deploy:cf": "pnpm build && npx wrangler pages deploy .output/public",
    "start": "node .output/server/index.mjs",
    "start:generate": "npx serve .output/public",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck"
  },
  "dependencies": {
    "@clerk/nuxt": "^1.10.0",
    "@floating-ui/vue": "^1.1.9",
    "@iconify-json/cib": "^1.2.3",
    "@sentry/nuxt": "^10.20.0",
    "@talex-touch/utils": "workspace:*",
    "gsap": "^3.12.5",
    "jszip": "^3.10.1",
    "motion-v": "^1.7.2",
    "ogl": "^1.0.11",
    "simplex-noise": "^4.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.17.0",
    "@cloudflare/workers-types": "^4.20241218.0",
    "@iconify-json/carbon": "^1.2.10",
    "@iconify-json/twemoji": "^1.2.2",
    "@nuxt/content": "^3.6.3",
    "@nuxt/devtools": "^2.6.2",
    "@nuxt/eslint": "^1.5.2",
